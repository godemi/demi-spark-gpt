{
  "name": "demi-gpt",
  "version": "2.3.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "start:watch": "func start -- --watch",
    "dev": "npm run prestart && concurrently \"npm:build:watch\" \"func start\"",
    "build:watch": "tsc --watch",
    "predeploy": "npm run clean && npm run build",
    "deploy": "func azure functionapp publish demigpt",
    "test": "vitest run",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration",
    "test:contract": "vitest run src/__tests__/contract",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage --reporter=json --outputFile=coverage.json",
    "publish:azure": "bash ./publish_azure_function.sh",
    "publish:azure:force": "yes | bash ./publish_azure_function.sh",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "smart-changeset": "ts-node src/scripts/smart-changeset.ts"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@azure/identity": "^4.5.0",
    "@prisma/client": "^6.6.0",
    "applicationinsights": "^3.4.0",
    "axios": "^1.8.2",
    "axios-retry": "^4.5.0",
    "changeset": "^0.2.6",
    "dotenv": "^16.4.7",
    "inquirer": "^12.5.2",
    "openai": "^4.76.0",
    "prisma": "^6.6.0",
    "stream": "^0.0.3",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "@types/semver": "^7.7.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "azure-functions-core-tools": "^4.5.0",
    "concurrently": "^9.1.2",
    "enquirer": "^2.4.1",
    "eslint": "^9.39.2",
    "execa": "^9.5.2",
    "msw": "^2.7.0",
    "nock": "^13.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.0",
    "semver": "^7.7.1",
    "supertest": "^7.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.50.0",
    "vitest": "^2.1.0"
  }
}
